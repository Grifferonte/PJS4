package accesBDD;

import Partage.utilisateur;

public abstract class AbstractUtilisateur implements utilisateur {
	
	private int id;
	private String login;
	private String pseudo;
	private int idStockage;

	public AbstractUtilisateur(String id, String mail, String pseudo, int idStockage) {
		this.id = id;
		this.login = mail;
		this
	}

	public int getId() {
		return this.id;
	}

	@Override
	public String getLogin() {
		return this.login;
	}

	@Override
	public void CréerProjet() {
	}

}
