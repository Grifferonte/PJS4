<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>EssaiCo</servlet-name>
    <servlet-class>MyServlet.connexion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EssaiCo</servlet-name>
    <url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>MyEntryServlet</servlet-name>
    <servlet-class>MyServlet.EntreeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyEntryServlet</servlet-name>
    <url-pattern>/Entry</url-pattern>
  </servlet-mapping>
</web-app>